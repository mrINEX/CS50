#include <stdio.h>
#include <cs50.h>

int main(void)
{
  for(int i=0; i<5; i++)
  {
      int min=i;
      for(j=j+1;j<5;j++)
      {
        if(arr[j]<arr[min])
        min=j;
      }
      if(min!=i)
      {
        int swap;
        swap=arr[i];
        arr[i]=arr[min];
        arr[min]=swap;
      }
  }
}
